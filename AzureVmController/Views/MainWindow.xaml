<Window x:Class="Com.revo.AzureVmController.Views.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Views="clr-namespace:Com.revo.AzureVmController.Views"
        xmlns:Converters="clr-namespace:Com.revo.AzureVmController.Converters"
		mc:Ignorable="d"
		Title="Azure VM controller" 
		Height="Auto" MinHeight="200" MaxHeight="400" 
		Width="Auto" MinWidth="300" MaxWidth="500"
		Icon="../cloud.ico" 
		ShowInTaskbar="False" 
		Topmost="True" 
		WindowStyle="None" 
		ResizeMode="NoResize" 
		SizeToContent="WidthAndHeight"
		WindowStartupLocation="Manual"
		Activated="MainWindow_OnActivated"
		SizeChanged="MainWindow_OnSizeChanged"
        DataContext="{Binding RelativeSource={RelativeSource Self}, Path=ViewModel}"
		>
	<Window.Resources>
		<CollectionViewSource x:Key="VmItemsSource" Source="{Binding VmItems}" d:Source="{d:DesignData ../DesignerSampleData/VmListItemSamples.xaml}" />
		<Converters:BoolInverter x:Key="BoolInverter"/>
	</Window.Resources>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*" />
			<RowDefinition Height="Auto"></RowDefinition>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="*" />
			<ColumnDefinition Width="Auto"/>
		</Grid.ColumnDefinitions>
		<Grid.Style>
			<Style TargetType="Grid">
				<Setter Property="Cursor" Value=""></Setter>
				<Style.Triggers>
					<DataTrigger Binding="{Binding IsUpdating}" Value="True">
						<Setter Property="Cursor" Value="Wait" />
					</DataTrigger>
				</Style.Triggers>
			</Style>
		</Grid.Style>
		<DockPanel 
			Grid.Row ="0"
			Grid.Column="0"
			Grid.ColumnSpan="2">
			<ListView ItemsSource="{Binding Source={StaticResource VmItemsSource}}" 
		         Height="Auto"
			     HorizontalContentAlignment="Stretch"
				 IsEnabled="{Binding Path=IsUpdating, Converter={StaticResource BoolInverter}}">
				<ListView.ItemTemplate>
					<DataTemplate>
						<DataTemplate.Triggers>
							<DataTrigger Binding="{Binding Busy}" Value="True">
								<Setter Property="Cursor" Value="AppStarting" />
							</DataTrigger>
							<DataTrigger Binding="{Binding Busy}" Value="False">
								<Setter Property="Cursor" Value="" />
							</DataTrigger>
						</DataTemplate.Triggers>
						<Border CornerRadius="6" BorderBrush="Black" BorderThickness="1" Padding="1,1,1,1">
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="*" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<StackPanel Grid.Column="0" Margin="2,0,5,0">
									<TextBlock Text="{Binding Name}" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20" FontStyle="Italic"/>
									<DockPanel>
									<TextBlock Text="{Binding OsType}" HorizontalAlignment="Left" />
									<TextBlock Text="{Binding State}" HorizontalAlignment="Right">
										<TextBlock.Style>
											<Style TargetType="TextBlock">
												<Setter Property="Foreground" Value="Black"></Setter>
												<Setter Property="FontStyle" Value="Normal"></Setter>
												<Style.Triggers>
													<DataTrigger Binding="{Binding Busy}" Value="True">
														<Setter Property="FontStyle" Value="Italic" />
														<Setter Property="Foreground" Value="DarkGray" />
													</DataTrigger>
													<DataTrigger Binding="{Binding State}" Value="Running">
														<Setter Property="Foreground" Value="Green" />
													</DataTrigger>
													<DataTrigger Binding="{Binding State}" Value="Stopped">
														<Setter Property="Foreground" Value="DarkOrange" />
													</DataTrigger>
													<DataTrigger Binding="{Binding State}" Value="Deallocated">
														<Setter Property="Foreground" Value="Red" />
													</DataTrigger>
												</Style.Triggers>
											</Style>
										</TextBlock.Style>
									</TextBlock>
								</DockPanel>
								</StackPanel>
								<StackPanel VerticalAlignment="Bottom" Grid.Column="1" Margin="0,0,0,0">
									<ProgressBar Margin="2,2,2,2" IsIndeterminate="True" Orientation="Horizontal" Height="10" Width="Auto">
										<ProgressBar.Style>
											<Style TargetType="ProgressBar">
												<Setter Property="Visibility" Value="Hidden" />
												<Style.Triggers>
													<DataTrigger Binding="{Binding Busy}" Value="True">
														<Setter Property="Visibility" Value="Visible" />
													</DataTrigger>
												</Style.Triggers>
											</Style>
										</ProgressBar.Style>
									</ProgressBar>
								<DockPanel >
									<Button Margin="2,2,2,2" IsEnabled="{Binding CanStart}" Click="Start_Clicked">Start</Button>
									<Button Margin="2,2,2,2" IsEnabled="{Binding CanStop}" Click="Stop_Clicked">Stop</Button>
									<Button Margin="2,2,2,2" IsEnabled="{Binding CanDeallocate}" Click="Deallocate_Clicked">Deallocate</Button>
								</DockPanel>
							</StackPanel>
							</Grid>
						</Border>
					</DataTemplate>
				</ListView.ItemTemplate>
			</ListView>
		</DockPanel>
		<ProgressBar Grid.Row="1" Grid.Column="0" 
					 VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch"
					 Orientation="Horizontal"					 					 
					 Margin="10,4,10,4"
					 IsIndeterminate="True">
			<ProgressBar.Style>
				<Style TargetType="ProgressBar">
					<Setter Property="Visibility" Value="Hidden" />
					<Style.Triggers>
						<DataTrigger Binding="{Binding IsUpdating}" Value="True">
							<Setter Property="Visibility" Value="Visible" />
						</DataTrigger>
					</Style.Triggers>
				</Style>
			</ProgressBar.Style>
		</ProgressBar>
		<StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
			<Button HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="Refresh_Clicked"  Margin="2,2,2,2" IsEnabled="{Binding IsUpdating, Converter={StaticResource BoolInverter}}">Refresh</Button>
			<Button HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="Settings_Clicked" Margin="2,2,2,2" IsEnabled="{Binding IsUpdating, Converter={StaticResource BoolInverter}}">Settings...</Button>
			<Button HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="Exit_Clicked" Margin="2,2,2,2">Exit</Button>
		</StackPanel>
